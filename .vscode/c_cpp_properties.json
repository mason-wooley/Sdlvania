{
    "env": {
        "projectIncludes": [
            "${workspaceFolder}/",
            "${workspaceFolder}/src/include/"
        ]
    },
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${projectIncludes}"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "C:/MinGW/bin/g++.exe",
            "compilerArgs": [
                "-w"
            ]
        }
    ],
    "version": 4
}